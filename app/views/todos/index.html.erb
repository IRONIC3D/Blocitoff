<div class="container">
  <div class="row-fluid row-fluid-1">
    <span class="span12">
      <div class="container">
        <h1 class="heading">My List</h1>
      </div>
    </span>
  </div>
  <div class="row-fluid">
    <span class="span4"></span>
    <span class="span4">
      <div class="new-todo">
        <%= render partial: 'form', locals: { todo: @todo } %>
      </div>
    </span>
    <span class="span4"></span>
  </div>
  <div class="container container-1">
    <div class="row-fluid">
      <span class="span3"></span>
      <span class="span6">
        <div class="pull-center">
          
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>Discription</th>
                  <th>Days Left</th>
                  <th>Completed</th>
                </tr>
              </thead>
              <tbody class="js-todos">
                <%= render partial: 'todo', collection: @todos %>
              </tbody>
            </table>
          
        </div>
      </span>
      <span class="span3"></span>
    </div>
  </div>
</div>